<launch>

  <arg name="state_subscriber" default="iwos_state_joints_tuw"/>
  <arg name="command_subscriber" default="iwos_cmd_twist_with_orientation"/>
  <arg name="command_publisher" default="iwos_cmd_jointsiws"/>

  <node name="IWOS_COMMAND_CONVERTER" pkg="tuw_iwos_command_converter" type="command_converter_node.py">
    <remap from="iwos_state_joints_tuw" to="$(arg state_subscriber)"/>
    <remap from="iwos_cmd_twist" to="$(arg command_subscriber)"/>
    <remap from="iwos_cmd_joints" to="$(arg command_publisher)"/>
  </node>

</launch>